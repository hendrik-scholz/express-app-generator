{
  "name": "express-app-generator",
  "version": "1.0.0",
  "description": "Express App Generator",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/index.ts",
    "coverage:report": "nyc report --reporter=lcov --reporter=html",
    "lint": "tslint src/**/*.ts",
    "test": "nyc --silent mocha -r ts-node/register test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hendrik-scholz/express-app-generator.git"
  },
  "keywords": [
    "express",
    "expressjs",
    "generator"
  ],
  "author": "Hendrik Scholz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hendrik-scholz/express-app-generator/issues"
  },
  "homepage": "https://github.com/hendrik-scholz/express-app-generator#readme",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/js-beautify": "^1.13.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.1",
    "chai": "^4.3.4",
    "chai-exclude": "^2.0.3",
    "js-beautify": "^1.14.0",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "ts-node": "^10.0.0",
    "tslint": "^6.1.3",
    "tslint-config-airbnb-base": "^0.3.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@types/express": "^4.17.12",
    "express": "^4.17.1",
    "log4js": "^6.3.0"
  }
}
